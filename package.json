    "browserify",
    "dashboard",
    "backbone",
    "react",
    "jsx",
    "kss",
    "styleguide",
    "api",
    "handlebars",
    "lodash"
  ],
  "homepage": "https://github.com/larsonjj/generator-yeogurt",
  "bugs": "https://github.com/larsonjj/generator-yeogurt/issues",
  "author": {
    "name": "Jake Larson",
    "email": "jake.j.larson@gmail.com",
    "url": "https://github.com/larsonjj"
  },
  "main": "app/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/larsonjj/generator-yeogurt.git"
  },
  "scripts": {
    "test": "istanbul cover _mocha --report lcovonly -- test/**/*.js --reporter list --timeout 100000 && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "localtest": "mocha test/**/*.js --reporter spec",
    "localtest-report": "istanbul cover _mocha --report html -- test/**/*.js && open coverage/index.html"
  },
  "dependencies": {
    "yeoman-generator": "~0.16.0",
    "colors": "~0.6.2",
    "lodash": "~2.4.1"
  },
