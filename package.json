{
  "name": "generator-yeogurt",
  "version": "0.9.4",
  "description": "A Yeoman generator that creates a sensible structure for static sites and single page applications by using your favorite tools.",
  "keywords": [
    "yeoman-generator",
    "yeogurt",
    "frontend",
    "html5",
    "modernizr",
    "jade",
    "swig",
    "sass",
    "less",
    "requirejs",
    "browserify",
    "dashboard",
    "backbone",
    "react",
    "jsx",
    "kss",
    "styleguide",
    "api",
    "handlebars",
    "lodash"
  ],
  "homepage": "https://github.com/larsonjj/generator-yeogurt",
  "bugs": "https://github.com/larsonjj/generator-yeogurt/issues",
  "author": {
    "name": "Jake Larson",
    "email": "jake.j.larson@gmail.com",
    "url": "https://github.com/larsonjj"
  },
  "main": "app/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/larsonjj/generator-yeogurt.git"
  },
  "scripts": {
    "test": "istanbul cover _mocha --report lcovonly -- test/*.js --reporter list --timeout 100000 && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "localtest": "mocha test/*.js",
    "localtest-report": "istanbul cover _mocha --report html -- test/*.js && open coverage/app/index.html"
  },
  "dependencies": {
    "yeoman-generator": "~0.16.0",
    "colors": "~0.6.2"
  },
  "devDependencies": {
    "mocha": "1.17.0",
    "coveralls": "2.8.0",
    "mocha-lcov-reporter": "0.0.1",
    "istanbul": "0.2.6"
  },
  "peerDependencies": {
    "yo": ">=1.0.6"
    },
    "engines": {
    "node": ">=0.10.0",
    "npm": ">=1.2.10"
  },
  "licenses": [
    {
      "type": "MIT"
    }
  ]
}
