{
    "name": "<%= _.slugify(projectName) %>",
    "version": "0.0.1",
    "dependencies": {<% if (useFoundation || !ieSupport) { %>
        "jquery": "2.1.0",<% } else { %>
        "jquery": "1.11.1",<% } %><% if ((/Backbone/i).test(jsFramework)) { %>
        "backbone": "1.1.2",
        "lodash": "2.4.1",<% } %><% if (jsTemplate === 'React') { %>
        "es5-shim": "3.4.0",<% } %><% if (jsFramework === 'Backbone + React' && jsOption === 'RequireJS') { %>
        "backbone-react-component": "0.6.2",
        "jsx-requirejs-plugin": "https://github.com/philix/jsx-requirejs-plugin.git#0.2.1",<% } %><% if ((/Handlebars/i).test(jsTemplate)) { %>
        "handlebars": "1.3.0",<% } else if ((/Jade/i).test(jsTemplate)) { %>
        "jade": "1.3.1",<% } %><% if (useFoundation) { %>
        "foundation": "5.2.3",<% } %>
        "requirejs": "2.1.11"<% if (useModernizr) { %>,
        "modernizr": "2.8.1"<% } %><% if (cssOption === 'LESS' && useBootstrap || cssOption === 'None (Vanilla CSS)' && useBootstrap || jsOption === 'None (Vanilla JavaScript)' && cssOption !== 'SASS' && useBootstrap) { %>,
        "bootstrap": "3.1.1"<% } else if (cssOption === 'SASS' && useBootstrap) { %>,
        "bootstrap-sass-official": "3.2.0"<% } %><% if (cssOption === 'LESS') { %><% if (!useBootstrap) { %>,
        "normalize.css": "~3.0.1"<% } %><% if (useLesshat) { %>,
        "lesshat": "3.0.0"<% } %><% } %><% if (cssOption === 'SASS') { %><% if (!useBootstrap) { %>,
        "normalize-scss": "~3.0.1"<% } %><% if (useBourbon) { %>,
        "bourbon": "3.2.1"<% } %><% } %>,<% if (useFontAwesome) { %>
        "font-awesome": "4.1.0"<% } %><% if (ieSupport) { %>,
        "html5shiv": "3.7.2",
        "respond": "1.4.2"<% } %>
    },
  "devDependencies": {}
}