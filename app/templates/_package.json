{
    "name": "<%= _.slugify(projectName) %>",
    "version": "0.0.1",
    "dependencies": {},
    "devDependencies": {
        "grunt": "~0.4.2",
        "grunt-contrib-connect": "0.7.1",
        "grunt-contrib-watch": "0.6.1",
        "grunt-contrib-clean": "0.5.0",<% if (useKss) { %>
        "grunt-kss": "0.3.1",<% } %>
        "grunt-contrib-compress": "0.7.0",<% if (htmlOption === 'Jade') { %>
        "grunt-contrib-jade": "0.11.0",<% } else if (htmlOption === 'Swig') { %>
        "grunt-swig-templates": "~0.1.1",<% } %>
        "grunt-contrib-htmlmin": "0.3.0",<% if (jsOption ==='RequireJS') { %>
        "grunt-contrib-requirejs": "0.4.4",<% } else if (jsOption ==='Browserify') { %>
        "minifyify": "3.0.6",
        "grunt-browserify": "2.0.8",
        "grunt-exorcise": "0.1.0",<% } %>
        "grunt-usemin": "2.1.1",<% if (useDocker) { %>
        "grunt-docker": "0.0.8",<% } %>
        "grunt-concurrent": "0.5.0",<% if (jsTemplate === 'Lo-dash (Underscore)') { %>
        "grunt-contrib-jst": "0.5.1",<% } else if (jsTemplate === 'Handlebars') { %>
        "grunt-contrib-handlebars": "0.8.0",<% } else if (jsFramework === 'Backbone + React') { %>
        "react": "0.10.0",
        "grunt-react": "0.8.2",<% } %>
        "grunt-contrib-concat": "0.4.0",<% if (cssOption === 'None (Vanilla CSS)') { %>
        "grunt-contrib-cssmin": "0.9.0",<% } %>
        "grunt-contrib-imagemin": "0.7.0",<% if (jshint) { %>
        "grunt-contrib-jshint": "0.10.0",
        "jshint-stylish": "0.2.0",<% } %>
        "grunt-contrib-copy": "0.5.0",
        "grunt-contrib-uglify": "0.4.0",<% if (cssOption === 'SASS') { %>
        "grunt-sass": "0.12.1",<% } %><% if (cssOption === 'LESS') { %>
        "grunt-contrib-less": "0.11.0",<% } %>
        "grunt-open": "0.2.3",
        "grunt-svgmin": "0.4.0",
        "grunt-karma": "0.8.0",
        "grunt-newer": "0.6.1",<% if (cssOption === 'None (Vanilla CSS)') { %>
        "grunt-uncss": "0.3.2",<% } %>
        "include-all": "0.1.6",<% if (jsOption === 'Browserify') { %>
        "karma-browserify": "0.0.6",<% } else if (jsOption === 'RequireJS') { %>
        "karma-requirejs": "0.2.1",<% } %>
        "karma-phantomjs-launcher": "0.1.2",
        "karma-chrome-launcher": "0.1.2",<% if (testFramework === 'Jasmine') { %>
        "karma-jasmine": "0.1.5",<% } else if (testFramework === 'Mocha + Chai') { %>
        "karma-chai": "0.1.0",
        "karma-mocha": "0.1.3",<% } %>
        "karma-jasmine": "0.1.5",
        "jit-grunt": "0.6.0",
        "karma": "0.12.0",
        "lodash": "2.4.1",
        "time-grunt": "0.2.10",<% if (useDashboard) { %>
        "grunt-dashboard": "~0.1.2",<% } %>
        "bower": "~1.3.3"<% if (useFTP) { %>,
        "grunt-ftpush": "0.3.3"<% } %>
    },
    "engines": {
        "node": ">=0.10.0"
    }
}
