{
    "name": "<%= _.slugify(projectName) %>",
    "version": "0.0.1",
    "dependencies": {<% if (jsTemplate === 'React') { %>
        "backbone-react-component": "git+https://git@github.com/larsonjj/backbone-react-component.git"<% if (useServer) { %>,<% } %><% } %><% if (useServer) { %>
        "colors": "^0.6.2",
        "async": "^0.7.0",
        "bcrypt-nodejs": "^0.0.3",
        "body-parser": "^1.0.2",
        "compression": "^1.0.1",
        "serve-favicon": "2.0.1",
        "errorhandler": "^1.0.0",
        "express": "~4.6.0",
        "express-session": "~1.6.0",
        "lusca": "^1.0.0",<% if (dbOption === 'MongoDB') { %>
        "mongoose": "^3.8.8",
        "connect-mongo": "^0.4.0",<% } %>
        "morgan": "^1.0.0",
        "ejs": "~0.8.4",<% if ('MySQL'.indexOf(dbOption) > -1) { %>
        "mysql": "2.3.2",
        "connect-session-sequelize": "1.0.0",
        "sequelize": "1.7.9",<% } %>
        "uglify-js": "^2.4.12",
        "lodash": "~2.4.1"<% if (jsTemplate === 'React') { %>,
        "node-jsx": "0.11.0",
        "react": "0.11.0"<% } %><% if (htmlOption === 'Swig') { %>,
        "swig": "1.4.1"<% } %><% if (jsTemplate === 'Jade' || htmlOption === 'Jade') { %>,
        "jade": "1.3.1"<% } %><% if (jsTemplate === 'Handlebars') { %>,
        "handlebars": "1.3.0"<% } %><% } %>
    },
    "devDependencies": {
        "grunt": "~0.4.2",<% if (useServer) { %>
        "grunt-open": "~0.2.0",<% } else { %>
        "grunt-contrib-connect": "0.7.1",<% } %>
        "grunt-contrib-watch": "0.6.1",
        "grunt-contrib-clean": "0.5.0",<% if (useKss) { %>
        "grunt-kss": "0.3.1",<% } %>
        "grunt-contrib-compress": "0.7.0",<% if (htmlOption === 'Jade' || jsTemplate === 'Jade') { %>
        "grunt-contrib-jade": "0.12.0",<% } %><% if (htmlOption === 'Swig') { %>
        "grunt-swig-templates": "~0.1.1",<% } %>
        "grunt-contrib-htmlmin": "0.3.0",<% if (jsOption ==='RequireJS') { %>
        "grunt-contrib-requirejs": "0.4.4",<% } else if (jsOption ==='Browserify') { %>
        "minifyify": "3.0.11",
        "grunt-browserify": "2.1.3",
        "grunt-exorcise": "0.1.0",<% } %>
        "grunt-usemin": "2.1.1",<% if (useJsdoc) { %>
        "grunt-jsdoc": "0.5.6",<% } %>
        "grunt-concurrent": "0.5.0",<% if (jsTemplate === 'Lo-dash (Underscore)') { %>
        "grunt-contrib-jst": "0.5.1",<% } else if (jsTemplate === 'Handlebars') { %>
        "grunt-contrib-handlebars": "0.8.0",<% } else if (jsFramework === 'Backbone + React') { %>
        "grunt-react": "~0.9.0",<% } %>
        "grunt-contrib-concat": "0.4.0",<% if (cssOption === 'CSS') { %>
        "grunt-contrib-cssmin": "0.9.0",<% } %>
        "grunt-contrib-imagemin": "0.7.1",
        "grunt-pngmin": "0.6.1",<% if (jshint) { %>
        "grunt-contrib-jshint": "0.10.0",
        "jshint-stylish": "0.2.0",<% } %>
        "grunt-contrib-copy": "0.5.0",
        "grunt-contrib-uglify": "0.4.0",<% if (cssOption === 'Sass') { %>
        "grunt-sass": "0.14.0",<% } %><% if (cssOption === 'Less') { %>
        "grunt-contrib-less": "0.11.0",<% } %>
        "grunt-open": "0.2.3",
        "grunt-svgmin": "0.4.0",
        "grunt-injector": "0.5.3",
        "grunt-env": "0.4.1",
        "grunt-karma": "0.8.0",
        "grunt-newer": "0.6.1",
        "include-all": "0.1.6",<% if (jsOption === 'Browserify') { %>
        "karma-browserify": "0.0.6",<% } else if (jsOption === 'RequireJS') { %>
        "karma-requirejs": "0.2.1",<% } %>
        "karma-phantomjs-launcher": "0.1.2",
        "karma-chrome-launcher": "0.1.2",<% if (testFramework === 'Jasmine') { %>
        "karma-jasmine": "0.1.5",<% } else if (testFramework === 'Mocha + Chai') { %>
        "karma-chai": "0.1.0",
        "karma-mocha": "0.1.3",<% } %>
        "karma-jasmine": "0.1.5",
        "karma": "0.12.0",
        "lodash": "2.4.1",
        "time-grunt": "0.2.10",<% if (useDashboard) { %>
        "grunt-dashboard": "~0.1.2",<% } %>
        "bower": "~1.3.3",<% if (useFTP) { %>
        "grunt-ftpush": "0.3.3",<% } %>
        "jit-grunt": "0.6.0"<% if (useServer) { %>,
        "connect-livereload": "~0.3.0",
        "grunt-express-server": "~0.4.5"<% } %>
    },
    "scripts": {<% if (useServer) { %>
        "start": "grunt build && NODE_ENV=production node server.js"<% } %>
    },
    "engines": {
        "node": ">=0.10.0"
    }
}
